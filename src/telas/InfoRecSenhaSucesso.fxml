<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="150.0" prefWidth="380.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ControllerInfoRecSenhaSucesso">
   <children>
      <Pane prefHeight="150.0" prefWidth="380.0">
         <children>
            <ImageView accessibleText="Imagem símbolo de correto" fitHeight="39.0" fitWidth="39.0" layoutX="14.0" layoutY="33.0">
               <image>
                  <Image url="@../imagens/success.png" />
               </image>
            </ImageView>
            <Text accessibleText="Sua senha foi alterada! " focusTraversable="true" layoutX="101.0" layoutY="58.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Senha alterada com sucesso!">
               <font>
                  <Font size="14.0" />
               </font>
            </Text>
            <Button fx:id="btnEntrarAuto" accessibleText="Clicando aqui você entrará na aplicação e poderá escutar seu livro favorito!" layoutX="14.0" layoutY="97.0" minHeight="25.0" mnemonicParsing="false" onAction="#logarAutomaticamente" prefHeight="27.0" prefWidth="150.0" style="-fx-background-color: #b93026;" text="Entrar automaticamente" textFill="WHITE" />
            <Button fx:id="btnVoltarTelaLogin" accessibleText="Deseja voltar e fazer seu login manualmente com sua nova senha? Clique aqui." layoutX="215.0" layoutY="97.0" minHeight="25.0" mnemonicParsing="false" onAction="#VoltarTelaParaLogin" prefHeight="27.0" prefWidth="150.0" style="-fx-background-color: #b93026;" text="Voltar para Login" textFill="WHITE" />
         </children>
      </Pane>
   </children>
</AnchorPane>
